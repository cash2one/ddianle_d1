function AddLevelUpRewardInfo(index, value)
    if (value ~= nil) then 
        local strMaleItem = value["maleitem"];
        local strFemaleItem = value["femaleitem"]
        local money = value["money"];
        local bindmcoin = value["bindmcoin"];

        ReadLevelUpRewardInfo(index, strMaleItem, strFemaleItem, money, bindmcoin, false);
    end
end
	
function AddVIPLevelUpRewardInfo(index, value)
    if (value ~= nil) then 
        local strMaleItem = value["maleitem"];
        local strFemaleItem = value["femaleitem"]
        local money = value["money"];
        local bindmcoin = value["bindmcoin"];

        ReadLevelUpRewardInfo(index, strMaleItem, strFemaleItem, money, bindmcoin, true);
    end
end

function AddLevelUpRecommend(index, value)
    if (value ~= nil) then 
        local funcID = value["funcID"];
        local funcName = value["funcName"];

        ReadLevelUpRecommend(index, funcID, funcName);
    end
end

